#include <stdio.h>

int main() {
    int rows, cols;
    
    
    printf("Enter number of rows: ");
    scanf("%d", &rows);
    
    printf("Enter number of columns: ");
    scanf("%d", &cols);
    
    
    int num = 1;

    
    for (int i = 0; i < rows; i++) {
        for (int j = 0; j < cols; j++) {
            printf("%3d ", num); // Adjust spacing for neat alignment
            num++;
        }
        printf("\n"); // Move to the next line after each row
    }

    return 0;
}
